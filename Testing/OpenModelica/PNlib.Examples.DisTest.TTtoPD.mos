// name: PNlib.Examples.DisTest.TTtoPD
// keywords: PNlib, discrete
// status: correct

loadFile("../../PNlib/package.mo"); getErrorString();
simulate(PNlib.Examples.DisTest.TTtoPD); getErrorString();

vars := {
  "P1.t",
  "T1.active",
  "T1.fire"
};

(success, failVars) := OpenModelica.Scripting.diffSimulationResults("PNlib.Examples.DisTest.TTtoPD_res.mat",
  "../../ReferenceFiles/PNlib.Examples.DisTest.TTtoPD.mat",
  "PNlib.Examples.DisTest.TTtoPD_diff", vars=vars); getErrorString();

if success then
  print("Files Equal!");
else
  filterSimulationResults("PNlib.Examples.DisTest.TTtoPD_res.mat", "../../ReferenceFiles/PNlib.Examples.DisTest.TTtoPD_minimal.mat", vars, 0); getErrorString();
end if;

