// name: PNlib.Examples.DisTest.NoInputConflict
// keywords: PNlib, discrete
// status: correct

loadFile("../../PNlib/package.mo"); getErrorString();
simulate(PNlib.Examples.DisTest.NoInputConflict); getErrorString();

vars := {
  "P1.t",
  "T1.active",
  "T1.fire",
  "T2.active",
  "T2.fire"
};

res := OpenModelica.Scripting.compareSimulationResults("PNlib.Examples.DisTest.NoInputConflict_res.mat",
  "../../ReferenceFiles/PNlib.Examples.DisTest.NoInputConflict.mat",
  "PNlib.Examples.DisTest.NoInputConflict_diff.csv", 0.01, 0.0001, vars); getErrorString();

if res[1] <> "Files Equal!" then
  filterSimulationResults("PNlib.Examples.DisTest.NoInputConflict_res.mat", "../../ReferenceFiles/PNlib.Examples.DisTest.NoInputConflict_minimal.mat", vars, 0); getErrorString();
end if;

